void main(){
  // Estruturas condicionais
  double nota = 4.0;
  
  if(nota < 5.0){
    print("Exame. :/");
  } else if(nota == 10.0){
    print("Sucesso total. :D");
  } else {
    print("Sucesso. :]");
  }
  
  bool aprovado = true;
  String info;
  
  /*if(aprovado){
    info = "Aprovado";
  }else{
    info = "Reprovado";
  }*/
  info = aprovado ? "aprovado." : "reprovado";
  
  print("Status do aluno: $info");
}