void main(){
    // geralmente se utiliza quando se sabe a quantidade de repetições/incrementos
    // definição ; condição ; incremento
    for(int i = 1; i <= 10  ; i++){
      //print("Indice : $i");
    }
    
    // geralmente se utiliza quando não sabe quantas vezes vai repetir
    int j = 1;
    // enquanto (condição for válida) faz: 
    while(j <= 10){
      //print("Indice j: $j");
      j++;
    }
    
    //geralmente se utiliza quando é necessário executar ao menos uma vez
    int k = 1;
    do{
      print("Indice k: $k");
      k++;
    } while (k <= 10);
}